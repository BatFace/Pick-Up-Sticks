{
  "name": "pick_up_sticks",
  "version": "1.0.0",
  "description": "An HTML/JavaScript implementation of the game Pick-up-sticks.",
  "main": "app.js",
  "scripts": {
    "test": "mocha ./src/js/tests",
    "staticCopyOps": "mkdir -p dist && cp src/index.html dist/ & cp -R src/css dist & cp -R src/images dist",
    "babel": "babel src -d dist",
    "js": "browserify ./src/app.jsx -o ./dist/app.js -t",
    "build": "npm run staticCopyOps && npm run js",
    "src:watch": "onchange 'src/**/**' -v -- npm run build"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "babel-polyfill",
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BatFace/Pick-Up-Sticks.git"
  },
  "keywords": [
    "game"
  ],
  "author": "BatFace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BatFace/Pick-Up-Sticks/issues"
  },
  "homepage": "https://github.com/BatFace/Pick-Up-Sticks#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "chai": "^3.5.0",
    "d3": "3.5.5",
    "http-server": "^0.9.0",
    "jquery": "^2.1.3",
    "microevent": "^1.0.0",
    "mocha": "^3.2.0",
    "npm": "^4.1.2",
    "onchange": "^3.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  }
}
